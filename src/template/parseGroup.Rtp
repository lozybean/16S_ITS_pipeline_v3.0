data=read.table('@#{file_group}',header=F,sep="\t",check.names=F,quote="")
group=read.table('@#{file_group_name}',header=F,check.names=F,sep="\t",quote="")
numcol = ncol(data)
numrow  = nrow(data)
if((numcol-2)==numrow){
    for(i in 1:numrow){
        write.table(data[,c(1,2+i)],file="@#{file_group_name}/group[i,1]")
    }
}else{
    print("warning :please check @#{file_group} @#{file_group_name}")
}